{
  "expo": {
    "name": "MillefeuilleApp",
    "slug": "Millefeuille",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon-1024.png",
    "splash": {
      "image": "./assets/Icon-1024.png",
      "backgroundColor": "#ff7d59"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/Icon-1024.png",
      "package": "com.smartphoneAppsProject.arakappa.millefeuille",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
